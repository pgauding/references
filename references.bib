  author = {Faust, Katherine},
  month = oct,
  year = {2008},
  pages = {273-282},
  file = {/Users/patrickgauding/Zotero/storage/R5VM7Q95/Faust - 2008 - Triadic configurations in limited choice sociometr.pdf}
}

