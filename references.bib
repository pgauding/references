@article{seligman_food_2010,
  title = {Food {{Insecurity Is Associated}} with {{Chronic Disease}} among {{Low}}-{{Income NHANES Participants}}},
  volume = {140},
  issn = {0022-3166, 1541-6100},
  abstract = {Food insecurity refers to the inability to afford enough food for an active, healthy life. Numerous studies have shown associations between food insecurity and adverse health outcomes among children. Studies of the health effects of food insecurity among adults are more limited and generally focus on the association between food insecurity and self-reported disease. We therefore examined the association between food insecurity and clinical evidence of diet-sensitive chronic disease, including hypertension, hyperlipidemia, and diabetes. Our population-based sample included 5094 poor adults aged 18\textendash{}65 y participating in the NHANES (1999\textendash{}2004 waves). We estimated the association between food insecurity (assessed by the Food Security Survey Module) and self-reported or laboratory/examination evidence of diet-sensitive chronic disease using Poisson regression. We adjusted the models to account for differences in age, gender, race, educational attainment, and income. Food insecurity was associated with self-reported hypertension [adjusted relative risk (ARR) 1.20; 95\% CI, 1.04\textendash{}1.38] and hyperlipidemia (ARR 1.30; 95\% CI, 1.09\textendash{}1.55), but not diabetes (ARR 1.19; 95\% CI, 0.89\textendash{}1.58). Food insecurity was associated with laboratory or examination evidence of hypertension (ARR 1.21; 95\% CI, 1.04\textendash{}1.41) and diabetes (ARR 1.48; 95\% CI, 0.94\textendash{}2.32). The association with laboratory evidence of diabetes did not reach significance in the fully adjusted model unless we used a stricter definition of food insecurity (ARR 2.42; 95\% CI, 1.44\textendash{}4.08). These data show that food insecurity is associated with cardiovascular risk factors. Health policy discussions should focus increased attention on ability to afford high-quality foods for adults with or at risk for chronic disease. J. Nutr. 140: 304\textendash{}310, 2010.},
  language = {en},
  number = {2},
  journal = {The Journal of Nutrition},
  doi = {10.3945/jn.109.112573},
  author = {Seligman, Hilary K. and Laraia, Barbara A. and Kushel, Margot B.},
  month = feb,
  year = {2010},
  pages = {304-310},
  file = {/Users/patrickgauding/Zotero/storage/LDHT2UF9/Seligman et al. - 2010 - Food Insecurity Is Associated with Chronic Disease.pdf}
}

@article{townsend_food_2001,
  title = {Food {{Insecurity Is Positively Related}} to {{Overweight}} in {{Women}}},
  volume = {131},
  issn = {0022-3166, 1541-6100},
  abstract = {Although individuals with poor food security might be expected to have reduced food intake, and thus reduced body fat and less likelihood of being overweight, these associations have not been adequately studied. The purpose of the current study was to examine the relationship between food insecurity and overweight as measured by body mass index (BMI) using data from the nationally representative 1994 \textendash{}1996 Continuing Survey of Food Intakes by Individuals (CSFII). Overweight was defined as BMI Ͼ27.3 kg/m2 for women and 27.8 kg/m2 for men. Food insecurity was related to overweight status for women (n ϭ 4509, P Ͻ 0.0001), but not for men (n ϭ 4970, P ϭ 0.44). Excluding the 11 severely insecure women, the prevalence of overweight among women increased as food insecurity increased, from 34\% for those who were food secure (n ϭ 3447), to 41\% for those who were mildly food insecure (n ϭ 966) and to 52\% for those who were moderately food insecure (n ϭ 86). Food insecurity remained a significant predictor of overweight status, after adjustment for potentially confounding demographic and lifestyle variables (P Ͻ 0.01). In a logistic regression analysis, mildly insecure women were 30\% more likely to be overweight than those who were food secure [odds ratio (OR) 1.3, P ϭ 0.005]. Thus, food insecurity had an unexpected and paradoxical association with overweight status among women with a higher prevalence of overweight among the food insecure, and a resulting potential for increased incidence of obesity-related chronic diseases. Given that the rates of both overweight and food insecurity are on the rise, this research area warrants further investigation. J. Nutr. 131: 1738 \textendash{}1745, 2001.},
  language = {en},
  number = {6},
  journal = {The Journal of Nutrition},
  doi = {10.1093/jn/131.6.1738},
  author = {Townsend, Marilyn S. and Peerson, Janet and Love, Bradley and Achterberg, Cheryl and Murphy, Suzanne P.},
  month = jun,
  year = {2001},
  pages = {1738-1745},
  file = {/Users/patrickgauding/Zotero/storage/8Z2YVMPF/Townsend et al. - 2001 - Food Insecurity Is Positively Related to Overweigh.pdf}
}

