  doi = {10.1093/ijpor/edh069},
  author = {Nir, L.},
  month = feb,
  year = {2005},
  pages = {422-442},
  file = {/Users/patrickgauding/Zotero/storage/FP6YH2SU/Nir - 2005 - Ambivalent Social Networks and Their Consequences .pdf}
}

